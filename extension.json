{
	"name": "Iframe",
	"author": "Sigbert Klinke",
	"url": "https://www.mediawiki.org/wiki/Extension:Iframe",
	"description": "This extension creates an iframe to embed an external webpage",
	"version": "0.04",
	"license-name": "GPL-2.0+",
	"type": "other",
